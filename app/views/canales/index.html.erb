<h1>Listing canales</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @canales.each do |canale| %>
  <tr>
    <td><%= canale.nombre %></td>
    <td><%= link_to 'Show', canale %></td>
    <td><%= link_to 'Edit', edit_canale_path(canale) %></td>
    <td><%= link_to 'Destroy', canale, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Canale', new_canale_path %>
