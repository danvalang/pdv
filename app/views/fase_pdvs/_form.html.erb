<%= form_for(@fase_pdv) do |f| %>
  <% if @fase_pdv.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@fase_pdv.errors.count, "error") %> prohibited this fase_pdv from being saved:</h2>

      <ul>
      <% @fase_pdv.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>


  <%= render 'cadena' %>
  <div class="field">
    <%= f.label :id %><br />
    <%= @fase_pdv.id %>
  </div>
  <div class="field">
    <%= f.label :nombre %><br />
    <%= @fase_pdv.nombre %>
  </div>
  <div class="field">
    <%= f.label :curt %><br />
    <%= @fase_pdv.curt %>
  </div>
   <div class="field">
    <%= f.label "Direccion" %><br />
    <%= @fase_pdv.geocodificacion %>
  </div>
  <div class="field">
    <%= f.label :ref_1 %><br />
    <%= @fase_pdv.ref_1 %>
  </div>
  <div class="field">
    <%= f.label :ref_2 %><br />
    <%= @fase_pdv.ref_2 %>
  </div>
 

  <div class="field">
    <%= f.label :cadena %><br />
    <%= @fase_pdv.cadena %>
  </div>
  <div class="field">
    <%= f.label :status %><br />
